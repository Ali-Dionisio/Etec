		int idade, selecionado;
		
		idade = Integer.parseInt(JOptionPane.showInputDialog(null, "Digite sua idade", "Entrada", JOptionPane.QUESTION_MESSAGE));
		String regiao[] = {"Centro","Leste", "Norte","Norte","Oeste","Sul"};
